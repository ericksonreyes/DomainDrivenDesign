{
  "name": "ericksonreyes/domain-driven-design",
  "description": "Domain Driven Design Inspired Classes and Interfaces",
  "license": "MIT",
  "type": "library",
  "require": {
    "php": ">=7.2"
  },
  "require-dev": {
    "phing/phing": "^2.16",
    "fzaninotto/faker": "~1.4",
    "phpunit/phpunit": "~7.0",
    "mockery/mockery": "~1.0",
    "phpspec/phpspec": "^4.0",
    "leanphp/phpspec-code-coverage": "^4.2",
    "squizlabs/php_codesniffer": "^3.3",
    "phpmd/phpmd": "^2.6",
    "sebastian/phpcpd": "^4.1",
    "php-coveralls/php-coveralls": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "EricksonReyes\\": "src",
      "spec\\": "spec"
    },
    "psr-0": {
      "": [
        "src"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "bin-dir": "bin"
  },
  "extra": {
    "hooks": {
      "pre-commit": [
        "echo committing as $(git config user.name)",
        "./scripts/lint"
      ],
      "post-merge": [
        "echo 'post-merge started'",
        "./scripts/post-merge",
        "echo 'post-merge finished';"
      ]
    }
  }
}
